#include<iostream>
using namespace std;

int main()
{
	cout << "输入第一个矩阵A为："<<endl;
	int	arrA[2][3] = 
	{
		{1,2,3},
	    {4,5,6}
	};
	for (int i= 0; i < 2; i++)
	{
		for (int j = 0; j < 3; j++)
		{
			cout << arrA[i][j] << " ";
		}
		cout << endl;
	}
	

	cout << "输入第一个矩阵B为："<<endl;
	int arrB[2][3] = 
	{
		{1,2,3},
	    {4,5,6}
	};
	for (int i = 0; i < 2; i++)
	{
		for (int j = 0; j < 3; j++)
		{
			cout << arrB[i][j] << " ";
		}
		cout << endl;
	}
	


	cout << "矩阵A点乘矩阵B的结果为：" << endl;
	for (int i = 0; i < 2; i++)
	{
		for (int j = 0; j < 3; j++)
		{
			cout << " " <<arrA [i][j] * arrB[i][j];
		}
		cout << "\n";
	}
	return 0;
	
}
